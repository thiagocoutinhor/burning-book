{
  "name": "burning-books",
  "version": "0.1.1-alpha",
  "description": "Spark Notebook web application centered around ssh connection",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:thiagocoutinhor/burning-books.git"
  },
  "scripts": {
    "production": "docker-compose -f docker-compose-production build && docker-compose -f docker-compose-production up -d",
    "prestart": "docker-compose up -d",
    "start": "nodemon",
    "stop": "docker-compose stop"
  },
  "nodemonConfig": {
    "ignore": [
      "web/scripts/*"
    ]
  },
  "keywords": [
    "spark",
    "notebook"
  ],
  "author": "Thiago Coutinho do Rego",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "bootstrap": "^4.4.1",
    "chalk": "^3.0.0",
    "connect-mongo": "^3.2.0",
    "d3": "^5.15.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-socket.io-session": "^1.3.5",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.2",
    "moo": "^0.5.1",
    "popper.js": "^1.16.1",
    "socket.io": "^2.3.0",
    "ssh2-promise": "^0.1.6"
  }
}
