{
  "name": "burning-books",
  "version": "0.1.1-alpha",
  "description": "Spark Notebook web application centered around ssh connection",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:thiagocoutinhor/burning-books.git"
  },
  "scripts": {
    "build:server": "npm i --production",
    "build:client": "cd client && npm i --production && npm run build",
    "build": "npm run build:server && npm run build:client",
    "dev": "concurrently --kill-others -n server,client -c green,blue \"nodemon\" \"npm start --prefix client\"",
    "start": "node index.js"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "ignore": [
      "client/*"
    ]
  },
  "keywords": [
    "spark",
    "notebook"
  ],
  "author": "Thiago Coutinho do Rego",
  "license": "ISC",
  "dependencies": {
    "chalk": "^3.0.0",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9",
    "express-session": "^1.17.1",
    "express-socket.io-session": "^1.3.5",
    "moment": "^2.24.0",
    "mongoose": "^5.9.10",
    "socket.io": "^2.5.0",
    "ssh2-promise": "^0.1.6"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "nodemon": "^2.0.3"
  }
}
